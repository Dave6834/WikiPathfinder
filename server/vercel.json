{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": {
          "buildCommand": "npm run build",
          "outputDirectory": "dist"
        }
      }
    ],
    "rewrites": [
      { "source": "/(.*)", "destination": "/dist/index.js" }
    ],
    "functions": {
      "dist/index.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }
